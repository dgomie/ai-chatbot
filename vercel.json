{
    "version": 2,
    "builds": [
      {
        "src": "client/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/build/index.html"
      }
    ]
  }